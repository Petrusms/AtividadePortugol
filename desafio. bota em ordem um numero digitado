programa
{
	
	funcao inicio()
	{
      inteiro n1, n2

     escreva("Fale um nÃºmero qualquer inteiro: ")
     leia(n1)
      escreva("Fale um segundo nÃºmero qualquer inteiro: ")
     leia(n2)

     se(n1>n2){
     enquanto(n1!=n2){
     	escreva(n1," ")
     	n1--
      }
      escreva(n1,"\n")
     }
      senao{
        enquanto(n1!=n2){
      	escreva(n1," ")
      	n1++
       }
       escreva(n2)
       }
     
   } 
}
