programa {
  funcao inicio() {
    inteiro diciplinas = 6
    inteiro resul, temp = 100
    resul = temp/diciplinas
    escreva("O tempo de estudo de cada matéria é de ", resul, " minutos")
    resul = temp%diciplinas
    escreva(", e seu tempo de descanso sera de ", resul, " minutos", "\n")

  }
}

